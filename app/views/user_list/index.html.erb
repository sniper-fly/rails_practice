<h1>Listing Users</h1>
<table>
  <thead>
  <tr>
    <th>name</th>
    <th>email</th>
    <th>link</th>
  </tr>
  </thead>
  <tbody>
  <% @user_lists.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'show', user_list_url(user.id), class: "btn btn-info btn-sm" %></td>
    </tr>
  <% end %>
  </tbody>

</table>
